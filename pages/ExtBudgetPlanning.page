    			$selectTerr = $divSelect.find('select.territorycls'), s_index = $selectTerr.get(0).selectedIndex;

    		if( (typeof(budget_id) == 'undefined' || budget_id == '') && sy_index == 0){
				$btnSave.hide();
   				$btnNext.hide();
			}else{
				$btnSave.show();
   				$btnNext.show();
			}
    		
    	}
