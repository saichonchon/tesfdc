    <script type="text/javascript">
    //	"use strict";
    //	window.onload = initDocumentPage;
    	addListener(window, 'load', initDocumentPage);
    	
    	function addListener (element, event, fn) {
			if (window.attachEvent) {
				element.attachEvent('on' + event, fn);
			} else {
				element.addEventListener(event, fn, false);
			}
		}
    	
    	;function initDocumentPage(){
    		var $divBtn = $('#btndivid'), $divSelect = $('#selectdivid'),
    			$btnSave = $divBtn.find('input[type="button"].btnsavecls'), $btnNext = $divBtn.find('input[type="button"].btnnextcls'),
    			$selectTerr = $divSelect.find('select.territorycls'), $selectYear = $divSelect.find('select.yearcls'),
				terr_len = $selectTerr.find('option').length, year_len = $selectYear.find('option').length, budget_id = $.trim( $('#budgethiddid').val() );

			if( typeof(budget_id) == 'undefined' || budget_id == '' ){
				$btnSave.hide();
   				$btnNext.hide();
			}
			if(terr_len == 2 && budget_id == ''){
				$selectTerr.find('option').each(function(idx){
					$(this).removeAttr('selected');
					if(idx == 1){
						$(this).attr({'selected':'selected'});
					}
				});
				$selectTerr.change();
			}
			//start
			var mode = '{!mode}';
			if(mode == 'edit') {
				var fq1dId = $('[id*=QB_FQ1_D]').attr('id');
				var fq1dIp = $('[id*=QB_FQ1_P]').attr('id');
				var fq1dIc = $('[id*=QB_FQ2_C]').attr('id');
				var tbd = $('[id*=TB_TB_D]').attr('id');
				var tbp = $('[id*=TB_TB_P]').attr('id');
				var tbc = $('[id*=TB_TB_C]').attr('id');
				console.log(fq1dId);
				console.log(fq1dIp);
				console.log(fq1dIc);
				calQuarterBudget(tbd, fq1dId, 'FQ1');
				calQuarterBudget(tbp, fq1dIp, 'FQ1');
				calQuarterBudget(tbc, fq1dIc, 'FQ1');
			}
            orderAccountTable();
    	}
    	
    	;function orderAccountTable(){
            var list_order=[], $body=$('#accountTable tbody');
            $('#accountTable tbody tr.mainTr').each(function(){
                list_order.push(this);
            });
            $body.find('tr.mainTr').remove();
            list_order.sort(function(a, b){
                var aSort=$(a).attr('data-sort'), bSort=$(b).attr('data-sort');
                return (aSort > bSort ? 1 : -1);
            });
            $body.find('tr:first').before(list_order);
            list_order=[];
            $body.find('tr.mainTr').each(function(){
                var info=this.getAttribute('data-sort');
                $(this).after( $body.find('tr[data-info="'+info+'"]') );
            });
            console.log('complate');
        }
    
    	;function checkhasYearComplete(){
    		var $divBtn = $('#btndivid'), $btnSave = $divBtn.find('input[type="button"].btnsavecls'), 
    			$btnNext = $divBtn.find('input[type="button"].btnnextcls'),
    			$selectYear = $('#selectdivid select.yearcls'), s_index = $selectYear.get(0).selectedIndex;
   			$btnSave.hide();
			$btnNext.hide();
    		if(s_index != 0){
	   			$btnSave.show();
				$btnNext.show();
    		}
    	}
    	;function checkhasTerrComplete(){
    		var budget_id = $.trim( $('#budgethiddid').val() ), $divSelect = $('#selectdivid'), $divBtn = $('#btndivid'), 
    			$btnSave = $divBtn.find('input[type="button"].btnsavecls'), 
