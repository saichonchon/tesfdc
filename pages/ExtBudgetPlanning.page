<apex:page standardController="Budget__c" extensions="ExtBudgetPlanning" sidebar="false" action="{!checkBudgetFQValue}">
    <apex:stylesheet value="{!$Resource.BudgetPlanningCss}"/>
    <script src="/soap/ajax/22.0/connection.js" type="text/javascript"></script>
    <script src="/soap/ajax/22.0/apex.js" type="text/javascript"></script>   
    <apex:includeScript value="{!URLFOR($Resource.TEJqueryMiNi)}" />
    <apex:includeScript value="{!URLFOR($Resource.ExtBudgetPlanningLimitJS)}" />
    <apex:includeScript value="{!URLFOR($Resource.CommonJS)}" />
    <style type='text/css'>     
        body{background-color:#fff !important;}
        .hideTableListButton {
            background: url({!URLFOR($Resource.iconCollapse)}) no-repeat scroll 0 -0px transparent !important;
            height:15px !important;
            width:10px !important;
        } 
        .showTableListButton {
            background: url({!URLFOR($Resource.iconExpand)}) no-repeat scroll 0 -0px transparent !important;    
            height:15px !important;
            width:10px !important;
        }   
        .inlineTable .inlineTableTd {padding: 0px;} 
        .inlineTable .inlineTableTd .list tr td,.inlineTable .inlineTableTd .list tr th{height:24px;padding:0px 5px;}   
        .inlineTable .inlineTableTd .tableNoLeftBorder{min-width:950px;}
        
        div.container table#accountTable tbody tr.dataRow td.dataCell span{display:inline;vertical-align:middle;}
        div.container table#accountTable tbody tr.dataRow td.dataCell span.helpButton img,
        div.container table#accountTable tbody tr.dataRow td.dataCell span.helpButtonOn img{right:1px;position:relative;}
    </style>
    <apex:form id="budgetForm">
        
        <br/>
        <apex:pageBlock tabstyle="Budget__c" id="budgetPB" mode="maindetail">                                                   
            <apex:pageMessages id="messages" />
            <input type="hidden" value="{!format}" id="localFormat"/>
            <div id="btndivid" style="width:75%;text-align:center; display:{!IF(showNext,'none','')}" >
            	<input type="hidden" id="budgethiddid" value="{!budget.Id}" />
                <input type="button" class="btn" value="{!$Label.Edit}" style="display:{!IF(AND(mode=='view', showEdit), '', 'none')}" onclick="editBudget();" />
                <apex:actionFunction name="editBudget" action="{!editBudget}" status="statusLoad" rerender="budgetForm" oncomplete="conFormatNum('accountTable');" immediate="true"/>
                    
                <input type="button" class="btn btnsavecls " value="{!$Label.SaveButton}" style="display:{!IF(mode=='edit', '', 'none')}"  onclick="conUnformatNum('accountTable'); saveBudget();" />
                <apex:actionFunction name="saveBudget" action="{!saveBudget}" status="statusLoad" rerender="budgetForm" oncomplete="conFormatNum('accountTable');"/>
                  
                <input type="button" class="btn btnnextcls " value="{!$Label.Next}" style="display:{!IF(mode=='edit', '', 'none')}"  onclick="if(confirm('{!$Label.Break_Down_Confirm}')){conUnformatNum('accountTable'); saveBudgetAndNext();}" />
                <apex:actionFunction name="saveBudgetAndNext" action="{!saveBudgetAndNext}" status="statusLoad" rerender="budgetForm" oncomplete="conFormatNum('accountBDDataTable');"/>
                     
                <input type="button" class="btn" value="{!$Label.Back}" onclick="backToView();" style="display:{!IF(!invalidBudget,'','none')}"/>
                <apex:actionFunction name="backToView" action="{!backToView}" immediate="true"/>
<!-- 
                <input type="button" class="btn" value="{!$Label.Back}" onclick="backToView();" style="display:{!IF(invalidBudget,'','none')}"/>
                <apex:actionFunction name="backToView" action="{!backToBudgetPlanning}" immediate="true"/>
 -->
                                
                <input type="button" class="btn" value="{!$Label.Break_Down}" style="display:{!IF(AND(mode=='view', budget.id!=null, showEdit, sameTerriotry), '', 'none')}" onclick="goToBreakDown();" />
                <apex:actionFunction name="goToBreakDown" action="{!goToBreakDown}" status="statusLoad" rerender="budgetForm" oncomplete="conFormatNum('accountBDDataTable');" immediate="true"/>
                
                <apex:outputPanel rendered="{!AND(mode='edit', !isNew, sameTerriotry)}">
                    &nbsp;&nbsp;<b>{!$Label.Recalculate}</b><apex:inputCheckBox value="{!isRecalculate}" onclick="conUnformatNum('accountTable'); recalculateBudget();"/>
                    <apex:actionFunction name="recalculateBudget" action="{!recalculateBudget}" status="statusLoad" rerender="budgetTablePBSection,messages" oncomplete="conFormatNum('accountTable');"/>
                </apex:outputPanel> 
                <apex:actionStatus startText="" stopText="" id="statusLoad" onstart="$('#loading-curtain-div').show();" onstop="$('#loading-curtain-div').hide();"/>                       
            </div>  
            <div style="width:75%;text-align:center; " id="selectdivid">
                <apex:actionRegion >                                        
                <apex:PageBlockSection id="headerPBSection" rendered="{!!showNext}">
                    <apex:pageBlockSectionItem id="territoryItem">
                        <apex:outputLabel >{!$Label.Territory}</apex:outputLabel>
                        <apex:outputPanel >
                            <apex:selectList styleClass="territorycls" size="1" value="{!theTerritory}" disabled="{!IF(isNew, false, true)}" >
                                <apex:selectOptions value="{!list_territoryItems}" />                       
                                <apex:actionSupport event="onchange" rerender="budgetTablePBSection, headerPBSection,messages" status="statusLoad" action="{!selectBudgetYear}" oncomplete="conFormatNum('accountTable'); checkhasTerrComplete();"/>                                                             
                            </apex:selectList>
                        </apex:outputPanel>             
                    </apex:pageBlockSectionItem>                
                    
                    <apex:pageBlockSectionItem id="yearItem">
                        <apex:outputLabel >{!$Label.Year}</apex:outputLabel>
                        <apex:outputPanel >
                            <!-- <apex:outputText value="{!budget.Year__c}" rendered="{!!isNew}" /> -->
                            <apex:selectList styleClass="yearcls" size="1" value="{!budget.Year__c}" disabled="{!IF(isNew, false, true)}">
                                <apex:selectOptions value="{!list_yearItems}" />                                                                                            
                                <apex:actionSupport event="onchange" rerender="budgetTablePBSection, headerPBSection,messages" status="statusLoad" action="{!selectBudgetTerritory}" oncomplete="conFormatNum('accountTable'); checkhasYearComplete();"/>    
                            </apex:selectList>      
                        </apex:outputPanel>                 
                    </apex:pageBlockSectionItem>                
                </apex:PageBlockSection>
                </apex:actionRegion>
            </div>
            <apex:PageBlockSection id="budgetTablePBSection" columns="1" rendered="{!!showNext}">
                <input id="otherBPJsonStrId" name="otherBPJsonStrId" type="hidden" value="{!otherBPJsonStr}" />
