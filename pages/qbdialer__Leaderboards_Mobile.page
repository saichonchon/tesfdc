<apex:page controller="qbdialer.InsideSalesSetup" showHeader="false" title="Dashboards" docType="html-5.0">
	<title>Dashboards</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
	<meta name="apple-mobile-web-app-capable" content="yes" />
	<!-- iPhone ICON -->
	<link href="{!URLFOR($Resource.c2c_resources, '/standings/leaderboards/appICO57.png')}" sizes="57x57" rel="apple-touch-icon" />
	<!-- iPad ICON-->
	<link href="{!URLFOR($Resource.c2c_resources, '/standings/leaderboards/appICO72.png')}" sizes="72x72" rel="apple-touch-icon" />
	<!-- iPhone (Retina) ICON-->
	<link href="{!URLFOR($Resource.c2c_resources, '/standings/leaderboards/appICO114.png')}" sizes="114x114" rel="apple-touch-icon" />
	<!-- iPad (Retina) ICON-->
	<link href="{!URLFOR($Resource.c2c_resources, '/standings/leaderboards/appICO144.png')}" sizes="144x144" rel="apple-touch-icon" />

	<!-- iPhone SPLASHSCREEN-->
	<link href="{!URLFOR($Resource.c2c_resources, '/standings/leaderboards/Final-320-460.png')}" media="(device-width: 320px)" rel="apple-touch-startup-image" />
	<!-- iPhone (Retina) SPLASHSCREEN-->
	<link href="{!URLFOR($Resource.c2c_resources, '/standings/leaderboards/Final-640-920.png')}" media="(device-width: 320px) and (-webkit-device-pixel-ratio: 2)" rel="apple-touch-startup-image" />
	<!-- iPad (portrait) SPLASHSCREEN-->
	<link href="{!URLFOR($Resource.c2c_resources, '/standings/leaderboards/Final-768-1004.png')}" media="(device-width: 768px) and (orientation: portrait)" rel="apple-touch-startup-image" />
	<!-- iPad (landscape) SPLASHSCREEN
	<link href="{!URLFOR($Resource.c2c_resources, '/standings/Final-320-460.png apple-touch-startup-image-748x1024.png')}" media="(device-width: 768px) and (orientation: landscape)" rel="apple-touch-startup-image">
	-->
	<!-- iPad (Retina, portrait) SPLASHSCREEN-->
	<link href="{!URLFOR($Resource.c2c_resources, '/standings/leaderboards/Final-1536-2008.png')}" media="(device-width: 1536px) and (orientation: portrait) and (-webkit-device-pixel-ratio: 2)" rel="apple-touch-startup-image" />
	<!-- iPad (Retina, landscape) SPLASHSCREEN-->
	<link href="{!URLFOR($Resource.c2c_resources, '/standings/leaderboards/Final-2048-1496.png')}" media="(device-width: 1536px)  and (orientation: landscape) and (-webkit-device-pixel-ratio: 2)" rel="apple-touch-startup-image" />
	<style type="text/css">
	/**/

	html, body, iframe, .holder {
		margin: 0px;
		width: 100%;
		height: 100%;
		max-width: 100% !important;
		overflow: hidden;
	}
	/**/
	</style>
	<div class="holder">
		<iframe
			scrolling="no"
			frameborder="0"
			marginheight="0"
			marginwidth="0"
			id="boardframe"
		></iframe>
	</div>
	<script type="text/javascript" src="{!URLFOR($Resource.c2c_resources, '/lib/jquery.min.js')}"></script>
	<script type="text/javascript" src="{!URLFOR($Resource.c2c_resources, '/lib/jquery-migrate.min.js')}"></script>
	<script type="text/javascript">
	/**/
	var setiframedims, iframe = document.getElementById("boardframe");
	//iframe.src = "/admin/standings/dashboard";

	window.addEventListener("orientationchange", function() {
		if (iframe && iframe.contentWindow && iframe.contentWindow.postMessage) {
			iframe.contentWindow.postMessage({
				orientation: window.orientation
			}, '{!JSENCODE(InsideSalesHostSmart)}');
		}
	}, false);
	
	var user_oauth_token = '{!JSENCODE($User.is_token__c)}';
	var user_subdomain = '{!JSENCODE($User.is_subdomain__c)}';
	if (!user_oauth_token || !user_subdomain) { 
		window.location.href="/apex/InsideSales_OAuth_Setup";
	}

	var target_location = '{!JSENCODE(InsideSalesHostSmart)}';
    target_location = target_location.replace(/\/+$/, '');
	target_location += '/admin/standings/dashboard?token={!JSENCODE($User.is_token__c)}';
    if (!window.navigator.standalone && location.href.match("firsttime=1")) {
        history.pushState({}, "Leaderboards", location.pathname);
        target_location += "&firsttime=1";
    }
	document.getElementById('boardframe').src = target_location;
	/**/
	</script>
</apex:page>