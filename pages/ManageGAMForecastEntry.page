<script type="text/javascript"> 
    function toggle(id, link, headerRowToDisplay) {
        var e = document.getElementById(id);

        if(e.style.display == '') {
            e.style.display = 'none';
            link.innerHTML = '<img class="imageNoBorder" src="{!$Resource.expand}" alt="Show or Hide" onchange="blankPanel" />';
        } else {
            e.style.display = '';
            link.innerHTML = '<img class="imageNoBorder" src="{!$Resource.collapse}" alt="Show or Hide" onchange="blankPanel"/>';
        }
        
        var f = document.getElementById(headerRowToDisplay);
        if(f != null) {
            if(f.style.display == '') {
                f.style.display = 'none';
            } else {
                f.style.display = '';
            }
        }
    }
    function toggleAccount(id, acctNum, link) {
        var fullNameOfDiv = ''+id +''+acctNum;
        //alert('we are looking for the div called '+fullNameOfDiv);
        var e = document.getElementById(fullNameOfDiv);
        //alert('we have found '+e);

        if(e.style.display == '') {
            e.style.display = 'none';
            link.innerHTML = '<img class="imageNoBorder" src="{!$Resource.expand}" alt="Show or Hide" onchange="blankPanel"/>';
        } else {
            e.style.display = '';
            link.innerHTML = '<img class="imageNoBorder" src="{!$Resource.collapse}" alt="Show or Hide" onchange="blankPanel"/>';
        }
    }
</script> 

