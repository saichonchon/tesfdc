							                <td><apex:outputText value="{!item.ytdActualValue}"/></td>
							                <td><apex:outputText value="{!item.forecastValue}"/></td>
							                <td><apex:outputText value="{!item.apValue1}"/></td>
							                <td><apex:outputText value="{!item.yoyValue1}"/></td>
							                <td><apex:outputText value="{!item.apValue2}"/></td>
							                <td><apex:outputText value="{!item.yoyValue2}"/></td>
							                <td><apex:outputText value="{!item.apValue3}"/></td>
							                <td><apex:outputText value="{!item.yoyValue3}"/></td>
										</tr>
			        				</apex:repeat>
			        			</tbody>
			        		</table>
			        		<apex:outputPanel rendered="{!NOT(ISNULL(accountPlan.Revenue_Section_Last_Updated_By__c))}">
			        			<i>{!$Label.TE_Revenue_Section_Last_Updated_By}&nbsp;<apex:outputfield Value="{!accountPlan.Revenue_Section_Last_Updated_By__c}"/>&nbsp;on&nbsp;<apex:outputfield Value="{!accountPlan.Revenue_Section_Last_Updated__c}"/></i>
			        		</apex:outputPanel>	      
        				</apex:outputPanel>
		        		<apex:outputPanel rendered="{!list_tePerformance.size == 0}">
	        				<b><apex:outputLabel value="{!$Label.Nothing_To_View}"/></b>
	        			</apex:outputPanel>
        			</apex:outputPanel>
        		</apex:pageBlockSectionItem>
        	</apex:pageBlockSection>
       		<apex:outputPanel >
   				<apex:outputPanel id="CBC3Panel">
   					<table>
		               <tr><td ><div id="chart_div" style="width: 600px; height: 400px;"></div></td></tr>
		            </table>
       			</apex:outputPanel>
        	</apex:outputPanel>
			<script type="text/javascript" src="https://www.google.com/jsapi"></script>
		    <script type="text/javascript">
		      google.load("visualization", "1", {packages:["corechart"]});
		      google.setOnLoadCallback(drawChart);
		      function drawChart() {
		      	var cbcPart1 = '{!cbcPart1}';
		      	var cbcPart2 = '{!cbcPart2}';
		      	var cbcPart3 = '{!cbcPart3}';
		      	var cbcPart4 = '{!cbcPart4}';
		      	var cbcPart5 = '{!cbcPart5}';
		      	
		      	//Cbc charts
		      	var data = new google.visualization.DataTable();
		      	data.addColumn('string', 'CBC3');
		  		data.addColumn('number', 'Amount in USD');
		      	if(cbcPart5 != null){
			      	data.addRows([
			          ['{!cbcTop1}',  parseFloat('{!cbcPart1}')],
			          ['{!cbcTop2}',  parseFloat('{!cbcPart2}')],
			          ['{!cbcTop3}',  parseFloat('{!cbcPart3}')],
			          ['{!cbcTop4}',  parseFloat('{!cbcPart4}')],
			          ['{!cbcTop5}',  parseFloat('{!cbcPart5}')]
			        ]);
		      	}
		      	else if(cbcPart4 != null){
			      	data.addRows([
			          ['{!cbcTop1}',  parseFloat('{!cbcPart1}')],
			          ['{!cbcTop2}',  parseFloat('{!cbcPart2}')],
			          ['{!cbcTop3}',  parseFloat('{!cbcPart3}')],
			          ['{!cbcTop4}',  parseFloat('{!cbcPart4}')]
			        ]);
		      	}
		      	else if(cbcPart3 != null){
			      	data.addRows([
			          ['{!cbcTop1}',  parseFloat('{!cbcPart1}')],
			          ['{!cbcTop2}',  parseFloat('{!cbcPart2}')],
			          ['{!cbcTop3}',  parseFloat('{!cbcPart3}')]
			        ]);
		      	}
		      	else if(cbcPart2 != null){
			      	data.addRows([
			          ['{!cbcTop1}',  parseFloat('{!cbcPart1}')],
			          ['{!cbcTop2}',  parseFloat('{!cbcPart2}')]
			        ]);
		      	}
		      	else if(cbcPart1 != null){
			      	data.addRows([
			          ['{!cbcTop1}',  parseFloat('{!cbcPart1}')]
			        ]);
		      	}
		        
				if(cbcPart1 != null){
					var options = {
			          title: 'Sales Breakdown',
			          backgroundColor: 'transparent',
			          is3D: 'true'
			        };
					var divId = document.getElementById('chart_div');
			        var chart = new google.visualization.PieChart(divId);
			        chart.draw(data, options);
				}
		      }
		    </script>	        	
	        	
