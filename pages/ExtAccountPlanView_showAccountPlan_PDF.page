<head>
    <apex:stylesheet value="{!URLFOR($Resource.ITB_Table_StyleNew)}" /> 
    <style type="text/css">
        @page { size: A4; }
        html, body { width: 100%; height: 100%; margin: 0; padding: 0; }
        .tdbg{
            background:#ADD8E6 !important;
        }
        .tar{text-align: left;}
        .tdline{
            border-bottom: 1px solid #ECECEC;
        }
        .sectionhead{
            background-color: #8BBEC1; border-color: #8BBEC1; line-height:15px; height: 15px;font-weight: bold;margin-top: 15px; overflow:hidden; color:#fff !important; border-width: 2px 0 0;border-style: solid; padding: 4px 3px;margin-bottom: 2px;width: 100%;
        }
        .sectionimg{
            background: rgba(0, 0, 0, 0) url('/img/alohaSkin/twisty_sprite.png') 0 -11px no-repeat;
            background-repeat: no-repeat; height: 11px;width: 11px;
            background-position: 1px -36px; margin-right: 4px;
        }
        .mess5titlecls{
            color: #DE8703; font-weight: bold; font-size: 16px; height: 21px; line-height: 21px; margin-top: 7px;
        }
        .h2 {text-align:center}
        #newsdiv_id  br{
            page-break-after:avoid !important; page-break-after:avoid;
        }
        #newsdiv_id  span{
            page-break-after:avoid !important; page-break-after:avoid;
        }
        #newsdiv_id  table,tbody,tr,td{
            page-break-after:avoid !important; page-break-after:avoid;
        }
        
    </style>
</head>
