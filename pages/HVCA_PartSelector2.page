            <script>
            function dependentPicklist(controlling_field_div_id,dependent_field_div_id,controlling_field_id,dependent_field_id){
                console.log('called');
                var MutationObserver = window.WebKitMutationObserver || window.MutationObserver || window.MozMutationObserver;
                var observer = new MutationObserver(function(mutations) {
                    // If the form-control was removed from the list â€“ re-assign it
                    if(!$('#'+controlling_field_div_id+ ' select').first().hasClass('form-control') || !$('#'+controlling_field_div_id+ '  select').first().hasClass('slds-input ')){
                        $('#'+controlling_field_div_id+ ' select').first().addClass('slds-input ');
                    }
                    if(!$('#'+dependent_field_div_id + ' select').first().hasClass('form-control')  || !$('#'+dependent_field_div_id + ' select').first().hasClass('slds-input ')){
                        $('#'+dependent_field_div_id + '  select').first().addClass('slds-input ');
                    }
                });
                
                var dependentlst1 = document.querySelector('#'+controlling_field_div_id);
                var dependentlst2 = document.querySelector('#'+dependent_field_div_id);
                observer.observe(dependentlst1, {
                    childList: true,
                    subtree: true,
                    attributes: true
                });
                observer.observe(dependentlst2, {
                    childList: true,
                    subtree: true,
                    attributes: true
                });               
            }
            </script>
