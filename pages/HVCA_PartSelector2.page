                                            <label class="slds-form-element__label" for="GIS_List_Price__c">Comment</label>
                                            <div id="GIS_Proposed_Price_Comment__c_div" class="slds-form-element__control">
                                                <apex:inputField id="GIS_Proposed_Price_Comment__c"   value="{!HVCA_Part_Selector__c.GIS_Proposed_Price_Comment__c}" styleClass="slds-input form-control " />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <apex:commandButton styleclass="btn btn-default"  action="{!saveRecord}" html-data-dismiss="modal" reRender="GIS-Output,top-button-section, bottom-button-section,status-section" value="Save" status="status"/>
                                    <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                                </div>
                            </div>                        
                        </div>
                    </div>
                    <!--------------------------------------------------- Modals Section ends here ---------------------------------------------------->
                </apex:form>   
                
                <apex:form id="attachment-block" >
                    <apex:outputPanel rendered="{!isError}">
                        <div class="alert alert-danger">
                            <apex:pageMessages >
                            </apex:pageMessages>
                            
                        </div>   
                    </apex:outputPanel>
                    <apex:outputPanel rendered="{!!isShowInputSecion}">
                        <div class=" slds-form--compound">  
                            <div class="panel panel-default">
                                <div id="isAttachmentSection" class="isAttachmentSection">
                                    <div class="panel-heading">
                                        <a data-toggle="collapse" data-target="#accessories" href="#accessories">                                                
                                            <span class="panel-heading-size"><b>Attachments </b></span>                                                
                                        </a>
                                    </div>  
                                    <div id="docOutput" class="panel-padding form-element__group panel-collapse collapse in" >                                         
                                        <apex:actionRegion >
                                            <apex:variable value="{!1}" var="count"/>
                                            <apex:repeat value="{!attachments}" var="attachment" >
                                                
                                                <div class="row">
                                                    <div class="col-md-6"><a href="/{!attachment.Id}" target="_blank"> {!count}) {!attachment.name}</a></div>
                                                    <div class="col-md-6">{!attachment.description}</div>                                                          
                                                </div>
                                                <apex:variable value="{!count+ 1}" var="count"/>
                                            </apex:repeat>
                                            <br/>
                                            <a href="javascript:;" onclick="toggleFileSection();" >Add a New File : </a><br/>
                                            <div class="row fileSection ">
                                                <div class="col-md-3"><apex:inputText value="{!attachment.name}" html-placeholder="Name Of File" id="fileName" styleClass=" slds-input"/> </div>
                                                <div class="col-md-3"><apex:inputFile value="{!attachment.body}" filename="{!attachment.name}" id="file" styleClass=" slds-input"/> </div>
                                                <div class="col-md-3"><apex:inputTextArea value="{!attachment.description}" html-placeholder="Description of File" id="description" styleClass=" slds-input"/></div>
                                                <div class="col-md-3"><apex:commandButton action="{!upload}" value="Attach File"  status="status" styleClass=" slds-button slds-button--neutral"   /></div>                                                    
                                            </div>                                      
                                        </apex:actionRegion>
                                    </div>
                                </div>
                                <script>
                                $('.isAttachmentSection').find('.panel-heading').css("color", "#333").css("background-color","#f5f5f5").css("border-color","#ddd").css("background-image", "linear-gradient(to bottom,#f5f5f5 0,#e8e8e8 100%)");                            
                                </script>
                            </div>
                        </div>
                        <br/>
                    </apex:outputPanel>
                </apex:form>
                
                
            </div>
            
        </body>
        <script>
        
        $( document ).ready(function() {
            console.log( "ready!" );
            onDocumentReady();
        });
        
        function onDocumentReady(){          
            fixSectionHeader();
            fixCheckbox();
        }   
        
        function fixSectionHeader(){
            $('.isShowTransitionSection').find('.panel-heading').css("color", "#333").css("background-color","#f5f5f5").css("border-color","#ddd").css("background-image", "linear-gradient(to bottom,#f5f5f5 0,#e8e8e8 100%)");
            $('.isShowSpliceSection').find('.panel-heading').css("color", "#333").css("background-color","#f5f5f5").css("border-color","#ddd").css("background-image", "linear-gradient(to bottom,#f5f5f5 0,#e8e8e8 100%)");
            $('.isShowGISSection').find('.panel-heading').css("color", "#333").css("background-color","#f5f5f5").css("border-color","#ddd").css("background-image", "linear-gradient(to bottom,#f5f5f5 0,#e8e8e8 100%)");
            $('.isAccessoriesSection').find('.panel-heading').css("color", "#333").css("background-color","#f5f5f5").css("border-color","#ddd").css("background-image", "linear-gradient(to bottom,#f5f5f5 0,#e8e8e8 100%)");
            $('.isAttachmentSection').find('.panel-heading').css("color", "#333").css("background-color","#f5f5f5").css("border-color","#ddd").css("background-image", "linear-gradient(to bottom,#f5f5f5 0,#e8e8e8 100%)");
        }
        
        function fixCheckbox(){
            if($('.termination-section').prop('checked')){
                $('#termination_checkbox').prop( "checked", true );
            }
            if($('.splice-section').prop('checked')){
                $('#splice_checkbox').prop( "checked", true );
            }
            if($('.gis-section').prop('checked')){
                $('#gis_checkbox').prop( "checked", true );
            }
            if($('.accessoriess-section').prop('checked')){
                $('#accessoriess_checkbox').prop( "checked", true );
            }
        }
        var body = $("html, body");
        
        function setStyle(search){ 
            fixCheckbox();      
            fixSectionHeader();
            if('{!isError}' == 'true'){
                body.stop().animate({scrollTop:0}, '500', 'swing' );   
            }  
            if(search == 'search'){
             body.stop().animate({scrollTop:0}, '500', 'swing' ); 
            }    
        }
        
        
        function populateInsulationOdValue(){     
            if($( 'input[id$=Insulation_Thickness__c]').val()!=undefined && $( 'input[id$=Insulation_Thickness__c]').val() != '' ){
                var insulationOd = parseFloat($( 'input[id$=Conductor_OD__c]').val()) + parseFloat(2*($('input[id$=Insulation_Thickness__c]').val()* 0.0254)); 
                insulationOd = insulationOd.toFixed(2);
                $('input[id$=Insulation_OD__c]').val(insulationOd);
            }
        }        
        
        function refreshForm(){
            fetchConductorOd(); 
        }
        
        function searchOnClick(){
            cableclamp($('.outer-jacket-od').val()); 
        }        
        
        function  setTerminationListPrice(){ 
           
            
            if('{!terminationOutputPriceTable}' != undefined && '{!terminationOutputPriceTable}' != ''){
                $('.termination-list-price').val('{!terminationOutputPriceTable.priceOutput.ELITE_DISTRIBUTOR__c}');
            }else{
            	 $('.termination-list-price').val(parseInt($('.termination-price').text()));   
            }   
         }
        
        function  setSpliceListPrice(){
            if('{!spliceoutputPriceTable}' != undefined && '{!spliceoutputPriceTable}'!=''){
                $('.splice-list-price').val('{!spliceoutputPriceTable.priceOutput.ELITE_DISTRIBUTOR__c}');
            }else{
            	 $('.splice-list-price').val(parseInt($('.splice-price').text()));   
            } 
         }
        function includeSplicePreQuote(){
            reRenderSpliceOutput();
        }   
        function includeGISPreQuote(){
            reRenderGISOutput();
        }
        function includeTerminationPreQuote(){
			reRenderTerminationOutput();            
        }
        
        </script>
    </html>
</apex:page>