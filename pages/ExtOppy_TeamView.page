<script type="text/javascript">
    function popupBackShow(){
        $("#popup-back").css("display","block");
    }
    function popupBackHidden(){
        $("#popup-back").css("display","none");
    }
    $(function(){
        $("table tbody tr:even").css("background","#f4f4f4");
        $("table tbody tr").click(function(){
            $("table tr").removeClass("trColor-click");
            $(this).addClass("trColor-click");
        });
        $("table tbody tr").hover(
            function(){$(this).addClass("trColor-sel");},
            function(){$(this).removeClass("trColor-sel");}
        );
        $(".popup-content-partDiv ul li div").click(function(){
            if (!$(this).hasClass("ulDiv-click")) {
            $(this).addClass("ulDiv-click");
            } else {
            $(this).removeClass("ulDiv-click");
            }
        });
        
        
        $(".popup-content-partDiv ul div").hover(function(){
            $(this).css("cursor","pointe");
        });
        $(".top input:eq(0)").click(function(){
            popupBackShow();
            $(".popup-Add").css("display","block");
        });
        $(".top input:eq(1)").click(function(){
            popupBackShow();
            $(".popup-Edit").css("display","block");
        });
        $(".popup-SearchClick").click(function(){
            $(".popup-Search").css("display","block");
        });
        
        $(".popup-Add .popup-button .popup-buttonGray").click(function(){
            $(".popup-Add").css("display","none");
            popupBackHidden();
        });
        $(".popup-Add .popup-button .popup-buttonYellow").click(function(){
            $(".popup-Add").css("display","none");
            popupBackHidden();
        });
        
        $(".popup-Edit .popup-button .popup-buttonGray").click(function(){
            $(".popup-Edit").css("display","none");
            popupBackHidden();
        });
        $(".popup-Edit .popup-button .popup-buttonYellow").click(function(){
            $(".popup-Edit").css("display","none");
            popupBackHidden();
        });
        
        $(".popup-Search .popup-button .popup-buttonGray").click(function(){
            $(".popup-Search").css("display","none");
            popupBackHidden();
        });
        $(".popup-Search .popup-button .popup-buttonYellow").click(function(){
            $(".popup-Search").css("display","none");
            popupBackHidden();
        });
        
        
        
    })
</script>
