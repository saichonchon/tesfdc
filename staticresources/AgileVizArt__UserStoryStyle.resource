       .hoverDetailWindow{
        	position: absolute;       	
        	display:none;
        	width:280px;
        	overflow:hidden;
        	min-width: 150px;
        	min-height:30px;
        	background-color: #f4f4f4;
        	height:auto;
        	border:1px solid #666;
        	z-index:9999;
        	left:-150px;
          	top:50px;
        	padding:8px;
        	border-radius:3px;
        	-webkit-box-shadow:3px 3px 2px #aaa;
        	-moz-box-shadow:3px 3px 2px #aaa;
        	float:left;
      		color:#333; 
      		word-wrap:break-word;
      		white-space:normal; 
      		-webkit-border-horizontal-spacing: 0px;
			-webkit-border-vertical-spacing: 0px;
			clear:inherit;
			font-family: Arial, Helvetica, sans-serif;
			font-size: 12px;
			font-weight: normal;
        }
 		.hoverDetailWindow  img{width:100%;height:auto;}
