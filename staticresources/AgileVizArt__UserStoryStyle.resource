.epicStory{ white-space:normal;word-break:break-all;background-color:#96d2ec;text-align:center;width:98%;padding:6px 0px; height:20px;vertical-align:middle; color:#ffffff; Font-family:arial; font-size:16px; font-weight:bold;border:#F8F8F8 0px groove;margin-bottom:10px; min-width:150px;text-overflow:ellipsis;overflow:hidden;line-height:150%;}
        .epicStory .link a{ color:#fff; text-decoration:none;}
        .epicStory .link a:hover{ color:#fff; text-decoration:underline;}
        .epicStory_column {position:relative; width: 98%;  padding-bottom: 20px;min-width:150px; min-height:650px;}
        .backlog_column { width: 100%; float: left; padding-bottom: 20px;min-width:150px; }
        .epicStory .ui-icon { float: right; margin-right:10px; }
        .portlet-title { margin: 0 1% 1em 1%; min-width:150px; }
        .portlet { position:relative;margin: 0 1% 1em 1%; min-width:150px;}
       
        .portlet-header { margin: 0.3em; padding-top: 2px; padding-left: 6px; text-align:left; font-size:12px; padding-right:0px;padding-bottom:2px;}        
        .portlet-header .ui-icon { float: right; margin-top:1px; margin-right:2px; margin-left:2px;}
        .portlet-header .title {text-overflow:ellipsis;white-space:nowrap;overflow:hidden;min-width:150px;word-wrap:normal;margin-top:2px;
        }       
        
        .portlet-header .title  a{white-space:nowrap;text-overflow:ellipsis;overflow:hidden;color:#fff; text-decoration:none;word-wrap:normal;margin-top:0px;}
        .portlet-header .title  a:hover{ color:#fff; text-decoration:underline;word-wrap:normal; margin-top:0px;}
        .portlet-content {padding:5px;text-align:left;word-wrap:break-word;white-space:normal;overflow:auto;font-weight:normal;color:#3333}
        .portlet-content span{float:left;width:100%;}
        .portlet-content span img{width:100%;height:auto;}
        .portlet-content u{text-decoration: none;}
        .portlet-content em{font-style: normal;}
        
        .portlet-content s{text-decoration: none;text-align: left;}
        .portlet-content a {text-decoration: none;text-align: left;}
        .portlet-content strike{text-decoration: none;text-align: left;}
        .portlet-content del{text-decoration: none;text-align: left;}
        .portlet-content div{text-align: left;}
        .ui-sortable-placeholder { border: 1px dotted black; visibility: visible !important; height: 50px !important; }
        .ui-sortable-placeholder * { visibility: hidden; }
        .portlet-header .effort-hours { float:right; height:18px; width:24px; color:#fff; background-color:#00A5D6; margin-top:1px; padding: 3px 0px 0px 0px;text-align:center; }
        .portlet-header .effort-hours_input { float:right; height:18px; width:47px; color:#000; background-color:#fff; margin-top:1px; padding: 0px 0px 0px 0px;text-align:center; border:1px solid #00A5D6;}
        .effort-hours-edit{width:35px;}
               
