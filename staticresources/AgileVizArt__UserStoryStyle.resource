        #mask {   
            position: absolute;   
            top: 0;   
            left: 0;   
            width:100%;   
            height:100%;    
            background: gray;   
            opacity:0.8;
            filter: ALPHA(opacity = 80);   
            z-index: 100;   
            visibility: hidden ;
            display:none;       
        }
        
