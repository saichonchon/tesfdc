<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Configuration Settings for mapping RTS Request code with fields (Created by Subba Reddy)</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>API_Name__c</fullName>
        <description>Stores the API Name of the field to be referenced.</description>
        <externalId>false</externalId>
        <inlineHelpText>API Name of the field to be referenced.</inlineHelpText>
        <label>API Name</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Code__c</fullName>
        <description>Represents the ATTRIBUTE_CDE in RTS Interface</description>
        <externalId>false</externalId>
        <inlineHelpText>Represents the ATTRIBUTE_CDE in RTS Interface</inlineHelpText>
        <label>Code</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Object_Name__c</fullName>
        <description>Name of the Object</description>
        <externalId>false</externalId>
        <inlineHelpText>API name of the object on which the field is present.</inlineHelpText>
        <label>Object Name</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>isItemAttribute__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Check this field if the field is OutputItemsAttribNode and is related to Item XML</description>
        <externalId>false</externalId>
        <inlineHelpText>Check this field if the field is OutputItemsAttribNode and is related to Item XML</inlineHelpText>
        <label>isItemAttribute</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>isProgramAttribute__c</fullName>
        <defaultValue>false</defaultValue>
        <description>Check this field if the field is OutputRtsAttribNode and is related to RTS_Request XML</description>
        <externalId>false</externalId>
        <inlineHelpText>Check this field if the field is OutputRtsAttribNode and is related to RTS_Request XML</inlineHelpText>
        <label>isProgramAttribute</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>RTS Mapping</label>
    <visibility>Public</visibility>
</CustomObject>
