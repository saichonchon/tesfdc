<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Use this to setup object types for PictureVizArt to update picture field ids in case related picture in attachment is accidentally deleted. Define object key prefix as name, i.e. first 3 digit of salesforce.com&apos;s object id. List all relevant picture id field API names as comma-separated list.</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>ITBpicture__Object_Name__c</fullName>
        <caseSensitive>false</caseSensitive>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Enter the object type&apos;s API name</inlineHelpText>
        <label>Object Name</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>ITBpicture__Picture_Field_Names__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>List all picture id API field names separated by comma</inlineHelpText>
        <label>Picture Field Names</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <label>Configuration</label>
    <visibility>Public</visibility>
</CustomObject>
