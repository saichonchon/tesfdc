        <fullName>IND_NPS_Func_Followup_create_validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND
( 
	RecordType.Name = &apos;NPS Task Industrial&apos;, 
	OR
	(
		AND(
			TEXT(Status) &lt;&gt; &apos;Completed&apos;, 
			TEXT(Status) &lt;&gt; &apos;Functional Follow Up Created&apos;,
			TEXT(PRIORVALUE(Status)) = &apos;Functional Follow Up Created&apos;
		), 
		AND(
			TEXT(PRIORVALUE(Status)) = &apos;Completed&apos;,
			TEXT(Status) != &apos;Completed&apos;
		)
	)
)</errorConditionFormula>
        <errorMessage>Task Can not be marked In Progress or Not Started after Functional Follow Up Tasks are created or Task is closed.</errorMessage>
