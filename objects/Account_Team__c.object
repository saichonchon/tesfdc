    <validationRules>
        <fullName>Create_Access_Validation</fullName>
        <active>true</active>
        <description>This validation rules avoids, that users not equal to GAM are allowed to create Account Team Member records.</description>
        <errorConditionFormula>AND($Profile.Name  &lt;&gt;  &quot;Service Account&quot;,
ISPICKVAL(Account__r.Type, &apos;Global Account&apos;),
$User.Salesforce_ID__c != Account__r.OwnerId,
$Profile.Name != &apos;Systemadministrator&apos;,
$Profile.Name != &apos;System Administrator&apos;
)</errorConditionFormula>
        <errorMessage>Only the GAM is allowed to create Account Team Members!!</errorMessage>
    </validationRules>
