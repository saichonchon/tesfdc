    <fields>
        <fullName>Team_Member_Formula__c</fullName>
        <description>This field is used to show the team members name in the related list. If a user is related the users name is shown, if a contact is related the contacts name is shown.

IF(
	NOT(
		ISBLANK(Team_Member__c)
	), 
	HYPERLINK(&quot;/&quot; &amp; Team_Member__c, Team_Member__r.FirstName &amp; &quot; &quot; &amp; Team_Member__r.LastName), 
	HYPERLINK(&quot;/&quot; &amp; Team_Member_Contact__c, Team_Member_Contact__r.FirstName  &amp; &quot; &quot; &amp; Team_Member_Contact__r.LastName)
)</description>
        <externalId>false</externalId>
        <formula>IF(
	NOT(
		ISBLANK(Team_Member__c)
	), 
	HYPERLINK(&quot;/&quot; &amp; Team_Member__c, Team_Member__r.FirstName &amp; &quot; &quot; &amp; Team_Member__r.LastName), 
	Team_Member_txt__c
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Team Member</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Team_Member_Key__c</fullName>
        <description>External Key for upserting account team members with their appropriate roles</description>
        <externalId>true</externalId>
        <inlineHelpText>Because there may be &gt; 1 Account Manager per account, need to include User Id as shown in Team Member text as part of the external Id.  Otherwise, uses team member reference, as this will be null for any team member who is not a salesforce user.</inlineHelpText>
        <label>Team Member Key</label>
        <length>100</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
