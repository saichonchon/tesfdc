<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>hold the data for the PM to be notified when certain interest tags are created.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Email__c</fullName>
        <description>email address of the person that wants to be notified</description>
        <externalId>false</externalId>
        <label>Email</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Email</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>GPL__c</fullName>
        <description>enter the GPL names for which the user requires notification. Multiple GPL separated by comma.</description>
        <externalId>false</externalId>
        <inlineHelpText>enter the GPL names for which the user requires notification. Multiple GPL separated by comma.</inlineHelpText>
        <label>GPL</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Regions__c</fullName>
        <description>Enter the region of the interest tag for which the user needs to be notified. Multiple regions separated by comma. 
Available values are: North America, EMEA, Greater China, India, Japan, Korea, Rest of APAC, Global</description>
        <externalId>false</externalId>
        <inlineHelpText>Multiple regions separated by comma. 
Available values are: Americas, EMEA (excl. India), China (AP Central), India, AP South, AP North,Australia / New Zealand, South America, Global</inlineHelpText>
        <label>Regions</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>User_ID__c</fullName>
        <description>holds the user ID of the person who needs to be notified</description>
        <externalId>false</externalId>
        <label>User ID</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>IND PM Notification Interest Tag</label>
    <visibility>Public</visibility>
</CustomObject>
