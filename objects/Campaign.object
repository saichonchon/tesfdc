    <validationRules>
        <fullName>Reject_Blank_BU</fullName>
        <active>true</active>
        <description>Reject any campaigns from Eloqua that have no business unit value</description>
        <errorConditionFormula>AND( ISNEW() , $User.Id = &quot;005E0000006AaBU&quot;, ISBLANK(TEXT(Business_Unit__c)))</errorConditionFormula>
        <errorMessage>Business Unit is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reject_non_C2S_Campaigns</fullName>
        <active>true</active>
        <description>Reject any campaigns that shouldn&apos;t be in this instance.  As BU Eloqua integrations are moved away from Central Org, this validation rule should be updated to accept any additional BUs.  Starting with Industrial.
11May18: updg the rule because of SMA-165</description>
        <errorConditionFormula>AND( $User.Id = &quot;005E0000006AaBU&quot;, 
NOT(CONTAINS( TEXT(Business_Unit__c) , &apos;Aerospace, Defense &amp; Marine&apos;)),
NOT(CONTAINS( TEXT(Business_Unit__c) , &apos;Appliances&apos;)),
NOT(CONTAINS( TEXT(Business_Unit__c) , &apos;Channel Business Unit&apos;)),
NOT(CONTAINS( TEXT(Business_Unit__c) , &apos;Consumer Devices&apos;)),
NOT(CONTAINS( TEXT(Business_Unit__c) , &apos;Corporate&apos;)),
NOT(CONTAINS( TEXT(Business_Unit__c) , &apos;Data and Devices&apos;)),
NOT(CONTAINS( TEXT(Business_Unit__c) , &apos;Energy&apos;)),
NOT(CONTAINS( TEXT(Business_Unit__c) , &apos;Industrial &amp; Commercial Transportations&apos;)),
NOT(CONTAINS( TEXT(Business_Unit__c) , &apos;Industrial-Automation &amp; Control&apos;)),
NOT(CONTAINS( TEXT(Business_Unit__c) , &apos;Industrial-Intelligent Buildings&apos;)),
NOT(CONTAINS( TEXT(Business_Unit__c) , &apos;Industrial-Lighting&apos;)),
NOT(CONTAINS( TEXT(Business_Unit__c) , &apos;Industrial-MEFA&apos;)),
NOT(CONTAINS( TEXT(Business_Unit__c) , &apos;Industrial-Railway&apos;)),
NOT(CONTAINS( TEXT(Business_Unit__c) , &apos;Medical Products&apos;)),
NOT(CONTAINS( TEXT(Business_Unit__c) , &apos;Sensors&apos;)),
NOT(CONTAINS( TEXT(Business_Unit__c) , &apos;Transportation-GATD&apos;)),
NOT(CONTAINS( TEXT(Business_Unit__c) , &apos;Transportation-ICT&apos;)),
NOT(CONTAINS( TEXT(Business_Unit__c) , &apos;Transportation-Sensors&apos;))
)</errorConditionFormula>
        <errorMessage>Selected Business Unit not currently supported in C2S</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Valid_Event_Dates</fullName>
        <active>true</active>
        <description>Event End Date must be greater or equal to the Event Start Date</description>
        <errorConditionFormula>Event_End_Date__c &lt; Event_Start_Date__c &amp;&amp;  ISPICKVAL( Business_Unit__c , &quot;Energy&quot;)</errorConditionFormula>
        <errorDisplayField>Event_End_Date__c</errorDisplayField>
        <errorMessage>Event End Date must be greater or equal to the Event Start Date</errorMessage>
    </validationRules>
