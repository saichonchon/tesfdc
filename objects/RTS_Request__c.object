        <fullName>RTS_Required_Fields_For_New_Customer</fullName>
        <active>true</active>
        <description>Please fill value for Industry Code, Country Code, Customer Ship to Name and Customer Location if the opportunity is coming from &apos;Prospect&apos; Account</description>
        <errorConditionFormula>AND($Setup.Datacomm_Profile_for_RTS__c.Is_Active__c ,  
IF(TEXT(New_Customer__c)=&quot;1&quot;,
OR( 
ISBLANK(Country_Code__c), 
ISBLANK(Industry_Code__c), 
ISBLANK(DTC_Customer_Location__c), ISBLANK(DTC_Customer_Ship_to_Name__c) ),false
)
)</errorConditionFormula>
        <errorMessage>Please fill value for Industry Code, Country Code, Customer Ship to Name and Customer Location if the opportunity is coming from &apos;Prospect&apos; Account</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RTS_required_Fields_For_Quote_Request</fullName>
        <active>false</active>
        <description>Quote Request Fields manadatory for DataComm Users</description>
        <errorConditionFormula>AND($Setup.Datacomm_Profile_for_RTS__c.Is_Active__c , ISPICKVAL(Request_Type__c,&quot;Engineering Quote Request&quot;), 
OR(ISPICKVAL(Product_Owning_GIBU__c,&quot;&quot;), ISPICKVAL(Request_Priority__c,&quot;&quot;), 
ISPICKVAL(Sales_Channel__c,&quot;&quot;),
ISPICKVAL(Pricing_Requirement__c,&quot;&quot;), ISBLANK(Request_Description__c)     
)
)</errorConditionFormula>
        <errorMessage>If this is a sample request, please change Request Type to &quot;Engineering Sample Request&quot;. If this is a quote request, please fill in all fields in the QUOTE REQUEST section of this form.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>RTS_required_Fields_For_Sample_Request</fullName>
        <active>true</active>
        <description>Sample Request Fields manadatory for DataComm Users</description>
        <errorConditionFormula>AND($Setup.DND_Profile_for_RTS__c.Is_Active__c , ISPICKVAL(Request_Type__c,&quot;Engineering Sample Request&quot;), 
OR( 
ISPICKVAL(Sample_Requester__c,&quot;&quot;),ISPICKVAL(ESR_Scope__c, &quot;&quot;), 
ISBLANK(Customer_PO_Number__c),ISBLANK(Comments_Ship_Instr_Reg_of_Consumpt__c) , 
ISPICKVAL(Manufacturing_Location_Plant__c,&quot;&quot;), 
ISPICKVAL(Department_Charge__c,&quot;&quot;) 
) 
)</errorConditionFormula>
        <errorMessage>Please fill in all fields in SAMPLE REQUEST section of this form.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Reason_For_Urgent_Validation</fullName>
        <active>true</active>
        <description>If the Request Priority is Urgent this field is required.</description>
        <errorConditionFormula>AND(
 ISPICKVAL(Request_Priority__c , &quot;Urgent&quot;) ,
 ISBLANK(Reason_for_Marking_as_Urgent__c ) ,
NOT($Profile.Name=&quot;Service Account&quot;)
 )</errorConditionFormula>
        <errorDisplayField>Reason_for_Marking_as_Urgent__c</errorDisplayField>
        <errorMessage>In case the request priority is urgent, Please specify Reason for Marking as Urgent.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Required_SF_Part_Number</fullName>
        <active>false</active>
        <description>Rule that requires a sample request type to have a SF Part Number.</description>
        <errorConditionFormula>AND( 

OR( 
$Profile.Name = &quot;DND Standard User&quot;, 
$Profile.Name = &quot;DND Engineering User w/Cost&quot;, 
$Profile.Name = &quot;DND FAE User&quot;), 

ISPICKVAL(Request_Type__c ,&quot;Engineering Sample Request&quot;), 
ISBLANK( Part__c ))</errorConditionFormula>
        <errorDisplayField>Part__c</errorDisplayField>
        <errorMessage>Please choose a Part Number from Lookup.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Select_if_ADM</fullName>
        <active>true</active>
        <description>If Product Owning GIBU is ADM, then a selection must be made</description>
        <errorConditionFormula>AND(
ISPICKVAL(Product_Owning_GIBU__c, &quot;Aerospace, Defense and Marine&quot;),
ISBLANK(TEXT(QR_Form_Type_Select__c)) 
)</errorConditionFormula>
        <errorDisplayField>QR_Form_Type_Select__c</errorDisplayField>
        <errorMessage>ADM users must select a QR form type. Please make a selection.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Send_to_RTS_Required</fullName>
        <active>true</active>
        <errorConditionFormula>( ISPICKVAL($User.GIBU__c, &quot;Data &amp; Devices&quot;) &amp;&amp; 
( Send_to_RTS__c=false) &amp;&amp; 
NOT($Profile.Name=&quot;Service Account&quot;) &amp;&amp; 
NOT($Profile.Name=&quot;Systemadministrator&quot;) &amp;&amp; 
NOT($Profile.Name=&quot;System Administrator&quot;) 
)</errorConditionFormula>
        <errorDisplayField>Send_to_RTS__c</errorDisplayField>
        <errorMessage>You must select Send to RTS to save this request.</errorMessage>
    </validationRules>
