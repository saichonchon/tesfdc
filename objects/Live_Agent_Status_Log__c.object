    <fields>
        <fullName>Hour_of_the_day_As_Per_GMT__c</fullName>
        <externalId>false</externalId>
        <formula>MID (Status_Change_Time_1__c , 12, 2)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Hour of the day(As Per GMT)</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Hour_of_the_day_As_Per_Local__c</fullName>
        <externalId>false</externalId>
        <formula>VALUE(MID (TEXT (CreatedDate - 0.1667), 12, 2))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Hour of the day(As Per Local)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Status_Change_Time_1__c</fullName>
        <externalId>false</externalId>
        <formula>SUBSTITUTE(TEXT(CreatedDate), &apos;Z&apos;, &apos;.&apos; + MID(TEXT(MOD(MOD(CreatedDate - $System.OriginDateTime, 1), 86400)), 3, 3))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Status Change Time 1</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
