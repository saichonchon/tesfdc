    <fields>
        <fullName>ACP_Base_BU__c</fullName>
        <externalId>false</externalId>
        <formula>IF(NOT(ISNULL(ACP_Past_year_Revenue__c)),ACP_Past_year_Revenue__c,0)  +
IF(NOT(ISNULL(ACP_LY_Market_Growth__c)),ACP_LY_Market_Growth__c,0)  +  
IF(NOT(ISNULL(ACP_LY_Price_Erosion__c)),ACP_LY_Price_Erosion__c,0)  +  
IF(NOT(ISNULL(ACP_Current_FY_Base__c)),ACP_Current_FY_Base__c,0)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Base BU</label>
        <precision>18</precision>
        <required>false</required>
