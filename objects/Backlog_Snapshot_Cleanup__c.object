<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>This custom setting holds the values for the deletion batch of Backlog Snapshot records</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Days_to_keep_CRD__c</fullName>
        <description>Description Number of days for which if the Customer Request Date is older then delete such record.</description>
        <externalId>false</externalId>
        <inlineHelpText>The value should be in number of days</inlineHelpText>
        <label>Days to keep Customer Request Date</label>
        <precision>4</precision>
        <required>true</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Days_to_keep_CSR__c</fullName>
        <description>Number of days for which if the Current Schedule Date is older then delete such record</description>
        <externalId>false</externalId>
        <inlineHelpText>The value should be in number of days</inlineHelpText>
        <label>Days to keep Current Schedule Date</label>
        <precision>4</precision>
        <required>true</required>
        <scale>0</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>Backlog Snapshot Cleanup</label>
    <visibility>Public</visibility>
</CustomObject>
