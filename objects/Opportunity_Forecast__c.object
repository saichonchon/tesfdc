    <fields>
        <fullName>Counts_for_IND_Conversion__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
AND(
(Part__r.BU_profit_center__c = &quot;IND Devices&quot; || Part__r.BU_profit_center__c = &quot;IND Systems&quot; ),

(Opportunity__r.RecordType.Name = &quot;Opportunity - Engineering Project&quot; || Opportunity__r.RecordType.Name = &quot;Opportunity - Sales Project&quot; || Opportunity__r.RecordType.Name = &quot;IND - Sales Project&quot; || Opportunity__r.RecordType.Name = &quot;IND - Engineering project&quot; || Opportunity__r.RecordType.Name = &quot;NDR Opportunity&quot; || Opportunity__r.RecordType.Name = &quot;CCR Opportunity&quot;|| Opportunity__r.RecordType.Name = &quot;Channel- Sales Opportunity&quot; || Opportunity__r.RecordType.Name = &quot;Channel- Engineering Opportunity&quot;),

(Not((ISPICKVAL(Opportunity__r.Type , &quot;Transfer existing business&quot;))||(ISPICKVAL(Opportunity__r.Type ,&quot;Defend Existing Business&quot;)))),

ISPICKVAL(Part__r.Status__c, &quot;Won&quot;),
(ISPICKVAL(Part__r.Process_Status__c, &quot;Production&quot;) || ISPICKVAL(Part__r.Process_Status__c, &quot;Ramp-down&quot;) || ISPICKVAL(Part__r.Process_Status__c, &quot;EOL&quot;)),

(Date__c &lt;= $Setup.IND_Fiscal_Date_Information__c.Last_date_of_fiscal_year__c &amp;&amp;
Date__c &gt;= $Setup.IND_Fiscal_Date_Information__c.First_Date_of_fiscal_year__c),


(Won_Date__c &lt;= $Setup.IND_Fiscal_Date_Information__c.Last_date_of_fiscal_year__c &amp;&amp;
Won_Date__c &gt;= $Setup.IND_Fiscal_Date_Information__c.First_Date_of_fiscal_year__c)
),
TRUE,FALSE
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Counts for IND Conversion</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Counts_for_IND_Pipeline__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
AND(
(Part__r.BU_profit_center__c = &quot;IND Devices&quot; || Part__r.BU_profit_center__c = &quot;IND Systems&quot; ),

(Opportunity__r.RecordType.Name = &quot;Opportunity - Engineering Project&quot; || Opportunity__r.RecordType.Name = &quot;Opportunity - Sales Project&quot; || Opportunity__r.RecordType.Name = &quot;IND - Sales Project&quot; || Opportunity__r.RecordType.Name = &quot;IND - Engineering project&quot; || Opportunity__r.RecordType.Name = &quot;NDR Opportunity&quot; || Opportunity__r.RecordType.Name = &quot;CCR Opportunity&quot;|| Opportunity__r.RecordType.Name = &quot;Channel- Sales Opportunity&quot; || Opportunity__r.RecordType.Name = &quot;Channel- Engineering Opportunity&quot;),

(Not((ISPICKVAL(Opportunity__r.Type , &quot;Transfer existing business&quot;))||(ISPICKVAL(Opportunity__r.Type ,&quot;Defend Existing Business&quot;)))),

(Date__c &lt;= (CASE( Month(Today()) , 10 , $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_1__c, 11, $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_1__c,12, $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_1__c, 1 , $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_2__c, 2, $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_2__c,3, $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_2__c,4 , $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_3__c,5 , $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_3__c,6 , $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_3__c,7 , $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_4__c,8 , $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_4__c,9 , $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_4__c,Today()) + 364)

&amp;&amp;

Date__c &gt;=(CASE( Month(Today()), 10 , $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_1__c, 11, $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_1__c,12, $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_1__c,1 , $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_2__c, 2, $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_2__c,3, $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_2__c,4 , $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_3__c,5 , $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_3__c,6 , $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_3__c,7 , $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_4__c,8 , $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_4__c,9 , $Setup.IND_Fiscal_Date_Information__c.Current_Fiscal_Quarter_4__c,Today()))),

OR(
OR(
AND(
OR(
ISPICKVAL(Part__r.Status__c, &quot;Weak&quot;),
ISPICKVAL(Part__r.Status__c, &quot;50/50&quot;),
ISPICKVAL(Part__r.Status__c, &quot;Likely&quot;),
ISPICKVAL(Part__r.Status__c, &quot;Commit&quot;)),
((TODAY() - DATEVALUE( Opportunity__r.Last_Modified_Date__c))&lt;61)
),
AND(
ISPICKVAL(Part__r.Status__c, &quot;Won&quot;),
ISPICKVAL(Part__r.Process_Status__c, &quot;Pre-prod&quot;),
((TODAY() - DATEVALUE( Opportunity__r.Last_Modified_Date__c ))&lt;61)
)

),
AND(
ISPICKVAL(Part__r.Status__c,&quot;Won&quot;),

( (Part__r.Won_Date__c &lt;= $Setup.IND_Fiscal_Date_Information__c.Last_date_of_fiscal_year__c &amp;&amp;
Part__r.Won_Date__c &gt;= $Setup.IND_Fiscal_Date_Information__c.First_Date_of_fiscal_year__c))
)
)
)

,TRUE,FALSE
)</formula>
