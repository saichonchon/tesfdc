<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Configure which roles from Market Manager Hierarchy will be included in Account Team Integration process.</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Account_Team_Role_Name__c</fullName>
        <description>Name of account team role that market manager hierarchy is being used for.</description>
        <externalId>false</externalId>
        <inlineHelpText>Ex. Tubing FE</inlineHelpText>
        <label>Account Team Role Name</label>
        <length>255</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Hierarchy_Code_List__c</fullName>
        <description>Comma separated list of Market Manager codes that this role should apply for.</description>
        <externalId>false</externalId>
        <inlineHelpText>Ex. D1234,D5678,D000</inlineHelpText>
        <label>Hierarchy Code List</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <label>Market Manager Hier Acct Team Roles</label>
    <visibility>Public</visibility>
</CustomObject>
