<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>Settings to exclude certain profiles from trigger based lead assignment. These assignments are handled by the ClsLeadRouteUtil class.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Exclude_Existing_Email_Assignment__c</fullName>
        <defaultValue>true</defaultValue>
        <description>Excludes this profile from having leads assigned to them even if they own a contact with the same email address as the lead</description>
        <externalId>false</externalId>
        <inlineHelpText>Excludes this profile from having leads assigned to them even if they own a contact with the same email address as the lead</inlineHelpText>
        <label>Exclude - Existing Email Assignment</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Exclude_Network_ID_Assignment__c</fullName>
        <defaultValue>true</defaultValue>
        <description>Excludes this profile from having leads assigned to them based on the Assignee Network ID lead field</description>
        <externalId>false</externalId>
        <inlineHelpText>Excludes this profile from having leads assigned to them based on the Assignee Network ID lead field</inlineHelpText>
        <label>Exclude - Network ID Assignment</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Profile_Id__c</fullName>
        <caseSensitive>true</caseSensitive>
        <description>15 character profile ID</description>
        <externalId>false</externalId>
        <inlineHelpText>15 character profile ID</inlineHelpText>
        <label>Profile Id</label>
        <length>15</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <label>Lead Routing Settings</label>
    <listViews>
        <fullName>All_Details</fullName>
        <columns>NAME</columns>
        <columns>OBJECT_ID</columns>
        <columns>SETUP_TYPE</columns>
        <columns>SETUP_OWNER_NAME</columns>
        <columns>Exclude_Existing_Email_Assignment__c</columns>
        <columns>Exclude_Network_ID_Assignment__c</columns>
        <columns>Profile_Id__c</columns>
        <columns>CREATED_DATE</columns>
        <columns>UPDATEDBY_USER.ALIAS</columns>
        <columns>UPDATEDBY_USER</columns>
        <columns>LAST_UPDATE</columns>
        <filterScope>Everything</filterScope>
        <label>All - Details</label>
        <language>en_US</language>
    </listViews>
    <visibility>Public</visibility>
</CustomObject>
