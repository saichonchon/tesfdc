<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>the settings for connections to the Realtime system</description>
    <enableEnhancedLookup>false</enableEnhancedLookup>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Certificate_name__c</fullName>
        <description>The Public name of the SSL sertificate used in this org</description>
        <externalId>false</externalId>
        <label>Certificate name</label>
        <length>100</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Traffic_Cop_Org_Name__c</fullName>
        <description>The routing name in the Realtime configuration</description>
        <externalId>false</externalId>
        <label>Traffic Cop Org Name</label>
        <length>20</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>active__c</fullName>
        <defaultValue>true</defaultValue>
        <description>is this configuration active</description>
        <externalId>false</externalId>
        <label>active</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>endpoint_url__c</fullName>
        <description>SOAP endpoint for the realtime process</description>
        <externalId>false</externalId>
        <label>endpoint url</label>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Url</type>
    </fields>
    <label>Real-Time Callout Settings</label>
    <visibility>Public</visibility>
</CustomObject>
