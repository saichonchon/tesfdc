{
if(records.length==0)
{
 alert(&quot;No participant in the list to request for Feedback&quot;);
}
else
{
for(var i=0;i&lt;records.length;i++)
{
   var thisP= new sforce.SObject(&quot;Learning_Response_Result__c&quot;);
   thisP.id = records[i].Id;
   thisP.Status_of_Participant__c=&quot;Feedback Requested&quot; 
   newRecords.push(thisP);
  
}

if(confirm(&quot;Do you want to Request Feedback from Attended Participants&quot;))
{

var result=sforce.connection.update([trainingRecord]);
var result2=sforce.connection.update(newRecords);

}
}
}
 
else
 {
