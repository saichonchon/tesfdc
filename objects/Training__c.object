    <validationRules>
        <fullName>Clender_invite_validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF( RecordType.DeveloperName==&apos;Training_with_Response_Date_Slots&apos; || RecordType.DeveloperName==&apos;Training_with_Response_Exam_Date_Slots&apos;, OR( ISBLANK( Email_of_Trainner__c ),ISBLANK( Location_of_Training__c ),ISBLANK(  Training_Room_URL__c )),false)</errorConditionFormula>
        <errorMessage>Please add an appropriate value to Location of Training, Email of Trainer, Training Room URL. These values are needed to generate the calender request to the user.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Slot_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF( RecordType.DeveloperName==&apos;Training_with_Response_Date_Slots&apos; || RecordType.DeveloperName==&apos;Training_with_Response_Exam_Date_Slots&apos; ,  AND( ISBLANK( Training_Date_and_Time1__c ) ,ISBLANK( Training_Date_and_Time2__c ),ISBLANK( Training_Date_and_Time4__c ),ISBLANK( Training_Date_and_Time3__c ),ISBLANK( Training_Date_and_Time5__c ),ISBLANK( Training_Date_and_Time6__c )) , False)</errorConditionFormula>
        <errorMessage>Please add at lest one Training Date Slot
     OR     
You can not cancel all the slots in one shot. At least one slot must be available for training.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Training_URL_validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(Training_by_Slides__c==True, AND( ISBLANK(  Training_Slide_URl1__c ) ,ISBLANK(  Training_Slide_URL2__c  ),ISBLANK(  Training_Slide_URL3__c  )) , False)</errorConditionFormula>
        <errorMessage>Please Enter at least one Training Slide URL.</errorMessage>
    </validationRules>
