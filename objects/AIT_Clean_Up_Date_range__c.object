<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>This custom setting is used for clean up activity  batch classes  to get the Interest tag object and Activity stage old records.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>Months_old__c</fullName>
        <description>This field is to hold the number of months,such that batch job fetch this many months old records and delete.</description>
