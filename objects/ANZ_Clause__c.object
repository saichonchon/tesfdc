        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/38.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/38.0/apex.js&quot;)}  

try{ 
   var opptyId = &apos;{!Opportunity.Id}&apos;,
       quoteType = &apos;{!Opportunity.Quote_Type__c }&apos;;
   var result = sforce.apex.execute(&quot;OpportunityResetClauses&quot;, &quot;resetClauses&quot;,{currentOppyId : opptyId , currentOppyQuoteType : quoteType });
   var calloutResult = JSON.parse(result);
   if(calloutResult.isSuccessful){
      window.location.reload();
   }else{
      alert(calloutResult.resultMessage);
   }
} 
catch(err) { 
txt=&quot;There was an error on this page.\n\n&quot;; 
txt+=&quot;Error description: &quot; + err.description + &quot;\n\n&quot;; 
txt+=&quot;Click OK to continue.\n\n&quot;; 
alert(txt); 
}</url>
