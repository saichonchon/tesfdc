    <fields>
        <fullName>Lifetime_in_Months__c</fullName>
        <externalId>false</externalId>
        <formula>(YEAR( End_Date__c )-YEAR( Start_Date_for_time_period__c ))*12+MONTH(End_Date__c)-MONTH(Start_Date_for_time_period__c)+1</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Lifetime in Months</label>
