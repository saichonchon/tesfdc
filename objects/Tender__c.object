    <validationRules>
        <fullName>Coumtry_code_for_country_of_service</fullName>
        <active>true</active>
        <description>This validation rule ensures that the user has to enter a 2-digit country code for the country of service.</description>
        <errorConditionFormula>LEN(  Country_of_Service__c  ) &lt;&gt; 2</errorConditionFormula>
        <errorDisplayField>Country_of_Service__c</errorDisplayField>
        <errorMessage>Only a 2-letter country code (E.g. CN = China, US = United States) is possible.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_Greater_Than_Manufacturing_Date</fullName>
        <active>true</active>
        <errorConditionFormula>End_Date__c &lt; ( Manufacturing_Start_Date__c  + 270 )</errorConditionFormula>
        <errorMessage>The End Date must be minimum 9 months later than the manufacturing start date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Number_of_Train_Carriages_validation</fullName>
        <active>true</active>
        <description>Validates the data enetered in No.Of Carriages Field, i.e should be morethan &quot;Zero&quot;.</description>
        <errorConditionFormula>Number_of_Train_Carriages__c  &lt;= 0</errorConditionFormula>
        <errorDisplayField>Number_of_Train_Carriages__c</errorDisplayField>
        <errorMessage>Train Carriage value should be Greater Than &quot;Zero&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Number_of_Trains_validation</fullName>
        <active>true</active>
        <description>Validates the data enetered in No.Of Trains Field i.e should be morethan &quot;Zero&quot;.</description>
        <errorConditionFormula>Number_of_Trains__c &lt;= 0</errorConditionFormula>
        <errorDisplayField>Number_of_Trains__c</errorDisplayField>
        <errorMessage>No . Of Trains value should be Greater Than &quot;Zero&quot;</errorMessage>
    </validationRules>
