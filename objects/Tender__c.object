        <fullName>Fiscal_Month_Start_Date_Time_Period__c</fullName>
        <externalId>false</externalId>
        <formula>IF( MONTH (Start_Date_for_time_period__c) &lt; 10, MONTH ( Start_Date_for_time_period__c )+3, MONTH ( Start_Date_for_time_period__c )-9)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Fiscal Month Start Date Time Period</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
