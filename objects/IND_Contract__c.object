    <validationRules>
        <fullName>IND_Contract_Account_Change</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

NOT(ISPICKVAL( Contract_Status__c , &quot;Admin Edit&quot;)),
NOT(ISCHANGED (Contract_Status__c)),

ISCHANGED(   Account__c  )

)</errorConditionFormula>
        <errorMessage>You are not allowed to change the account after initial setup. Please contact your Administrator for assistance.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IND_Contract_Admin_Edit</fullName>
        <active>true</active>
        <description>This validation rule will prevent an Admin from changing the dates without having chosen status = Admin Edit</description>
        <errorConditionFormula>AND(
OR(
$Profile.Name = &quot;BU Admin&quot;,
$Profile.Name = &quot;System Administrator&quot;,
$Profile.Name = &quot;Systemadministrator&quot;
),
NOT(ISPICKVAL( Contract_Status__c , &quot;Admin Edit&quot;)),
NOT(ISCHANGED (Contract_Status__c)),
OR(
ISCHANGED(Contract_Start_Date__c) ,
ISCHANGED( Contract_End_Date__c) ,
ISCHANGED( Cancel_Contract_XX_days_before_renewal__c) ,
ISCHANGED( Renewal_Term_Months__c),
ISCHANGED(  Date_for_next_review__c  ),
ISCHANGED(  TESOG_Contract_Number__c  )

) 
)</errorConditionFormula>
        <errorMessage>Please use the &apos;Edit Contract&apos; button to make changes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>IND_Contract_Record_Type</fullName>
        <active>false</active>
        <description>This validation rule will prevent an Admin from changing the record type</description>
        <errorConditionFormula>AND(
OR(
$Profile.Name = &quot;BU Admin&quot;,
$Profile.Name = &quot;System Administrator&quot;,
$Profile.Name = &quot;Systemadministrator&quot;
),
NOT(ISPICKVAL( Contract_Status__c , &quot;Admin Edit&quot;)),
NOT(ISCHANGED (Contract_Status__c)),

ISCHANGED(  RecordTypeId )

)</errorConditionFormula>
        <errorMessage>You are not allowed to change the record type</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Review_Date_in_Contract_End_Period</fullName>
        <active>true</active>
        <errorConditionFormula>IF(
AND(
NOT(ISBLANK(Contract_will_be_sent_for_approval_on__c)),
Date_for_next_review__c  &gt;=  Contract_will_be_sent_for_approval_on__c 
), true, false)</errorConditionFormula>
        <errorMessage>Your review date falls into the period before the end of the runtime, please choose an earlier date.</errorMessage>
    </validationRules>
