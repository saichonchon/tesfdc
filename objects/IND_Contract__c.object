    <fields>
        <fullName>Expiration_notice_will_be_sent_on__c</fullName>
        <externalId>false</externalId>
        <formula>IF(
OR(
AND(
 Yearly_review_always_required__c = true,
 Temp_End_date__c &lt;&gt;  End_Date_Renewal_Period__c 
),
AND(
 Yearly_review_always_required__c = true,
 ISBLANK(End_Date_Renewal_Period__c)
)
),

 Temp_End_date__c  -  Cancel_Contract_XX_days_before_renewal__c-90,null
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Next Yearly Action on:</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Date</type>
    </fields>
    <fields>
